        "vdslLineConfLineType" : {
            "nodetype" : "column",
            "moduleName" : "VDSL-LINE-MIB",
            "oid" : "1.3.6.1.2.1.10.97.1.1.11.1.45",
            "status" : "current",
            "syntax" : {
                "type" :                 {
                    "basetype" : "Enumeration",
                    "noChannel" : {
                        "nodetype" : "namednumber",
                        "number" : "1"
                    },
                    "fastOnly" : {
                        "nodetype" : "namednumber",
                        "number" : "2"
                    },
                    "interleavedOnly" : {
                        "nodetype" : "namednumber",
                        "number" : "3"
                    },
                    "fastOrInterleaved" : {
                        "nodetype" : "namednumber",
                        "number" : "4"
